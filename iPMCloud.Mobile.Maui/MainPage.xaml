                        <Image
                            Margin="0,0,0,0"
                            HorizontalOptions="FillAndExpand"
                            Source="dot2.png"
                            VerticalOptions="FillAndExpand" />
                    </StackLayout>
                    <StackLayout
                        Margin="1,-28,0,0"
                        Padding="0"
                        BackgroundColor="Transparent"
                        HorizontalOptions="CenterAndExpand"
                        Spacing="0"
                        VerticalOptions="CenterAndExpand">
                        <Label
                            x:Name="btn_StartPage_count"
                            Margin="0"
                            Padding="0"
                            FontSize="Default"
                            HorizontalOptions="CenterAndExpand"
                            HorizontalTextAlignment="Center"
                            Text=""
                            TextColor="#ffffff"
                            VerticalOptions="CenterAndExpand"
                            VerticalTextAlignment="Center" />
                    </StackLayout>
                </StackLayout>

                <Label
                    Margin="0"
                    FontSize="18"
                    HorizontalOptions="FillAndExpand"
                    HorizontalTextAlignment="Start"
                    LineBreakMode="TailTruncation"
                    Text="START"
                    TextColor="#ffffff"
                    VerticalOptions="Center" />
                <Border
                    x:Name="frm_img_LoginUser"
                    Margin="0,0,5,0"
                    Padding="0"
                    HeightRequest="40"
                    HorizontalOptions="End"
                    StrokeShape="RoundRectangle 20"
                    VerticalOptions="Center"
                    WidthRequest="40">
                    <Image
                        x:Name="img_LoginUser"
                        Margin="0"
                        HorizontalOptions="CenterAndExpand"
                        VerticalOptions="CenterAndExpand" />
                </Border>
                <StackLayout
                    Margin="0"
                    Padding="0"
                    HorizontalOptions="End"
                    Orientation="Vertical"
                    Spacing="0"
                    VerticalOptions="Center">
                    <Label
                        x:Name="lb_LoginUser"
                        Margin="0"
                        FontSize="14"
                        HorizontalOptions="Fill"
                        HorizontalTextAlignment="End"
                        LineBreakMode="TailTruncation"
                        Text=""
                        TextColor="#ffffff"
                        VerticalOptions="Center" />
                    <Label
                        x:Name="lb_LoginCustomer"
                        Margin="0"
                        FontSize="10"
                        HorizontalOptions="Fill"
                        HorizontalTextAlignment="End"
                        LineBreakMode="TailTruncation"
                        Text=""
                        TextColor="#ffffff"
                        VerticalOptions="Center" />
                </StackLayout>
