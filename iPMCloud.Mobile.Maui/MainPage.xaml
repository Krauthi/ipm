            <Grid
                Margin="0"
                Padding="10"
                ColumnSpacing="0"
                HeightRequest="48"
                HorizontalOptions="Fill"
                RowSpacing="0">

                <Grid.ColumnDefinitions>
                    <!--  links: Menübutton + Badge  -->
                    <ColumnDefinition Width="Auto" />
                    <!--  mitte: Titel  -->
                    <ColumnDefinition Width="*" />
                    <!--  rechts: Userbild  -->
                    <ColumnDefinition Width="Auto" />
                    <!--  rechts: Usertexte  -->
                    <ColumnDefinition Width="Auto" />
                </Grid.ColumnDefinitions>

                <!--  Menübutton  -->
                <Border
                    Grid.Column="0"
                    Margin="3,2,2,2"
                    Padding="1"
                    BackgroundColor="Transparent"
                    HorizontalOptions="Start"
                    Stroke="Transparent"
                    VerticalOptions="Center">
                    <Border.Shadow>
                        <Shadow
                            Brush="Black"
                            Opacity="0.3"
                            Radius="5"
                            Offset="2,2" />
                    </Border.Shadow>

                    <!--  klickbare Fläche  -->
                    <Grid
                        x:Name="btn_mainmenu"
                        Margin="0"
                        Padding="0"
                        BackgroundColor="Transparent"
                        HeightRequest="38"
                        HorizontalOptions="Start"
                        VerticalOptions="Center"
                        WidthRequest="38">

                        <Image
                            x:Name="btn_mainsettings_img"
                            Margin="0"
                            HeightRequest="30"
                            HorizontalOptions="Start"
                            VerticalOptions="Center"
                            WidthRequest="30" />
                    </Grid>
                </Border>

                <!--  Badge/Counter über dem Menübutton (Overlay)  -->
                <Grid
                    x:Name="btn_StartPage_frame_count"
                    Grid.Column="0"
                    Margin="-20,3,5,0"
                    HeightRequest="21"
                    HorizontalOptions="Start"
                    IsVisible="False"
                    VerticalOptions="Start"
                    WidthRequest="21">

                    <Image
                        HorizontalOptions="Fill"
                        Source="dot2.png"
                        VerticalOptions="Fill" />

                    <Label
                        x:Name="btn_StartPage_count"
                        Margin="0,0,0,0"
                        FontSize="Default"
                        HorizontalOptions="Center"
                        HorizontalTextAlignment="Center"
                        Text=""
                        TextColor="#ffffff"
                        VerticalOptions="Center"
                        VerticalTextAlignment="Center" />
                </Grid>

                <!--  Titel  -->
                <Label
                    Grid.Column="1"
                    Margin="0"
                    FontSize="18"
                    HorizontalOptions="Fill"
                    HorizontalTextAlignment="Start"
                    LineBreakMode="TailTruncation"
                    Text="START"
                    TextColor="#ffffff"
                    VerticalOptions="Center" />

                <!--  Userbild rechts  -->
                <Border
                    x:Name="frm_img_LoginUser"
                    Grid.Column="2"
                    Margin="0,0,5,0"
                    Padding="0"
                    HeightRequest="40"
                    HorizontalOptions="End"
                    StrokeShape="RoundRectangle 20"
                    VerticalOptions="Center"
                    WidthRequest="40">
                    <Image
                        x:Name="img_LoginUser"
                        Margin="0"
                        HorizontalOptions="Center"
                        VerticalOptions="Center" />
                </Border>

                <!--  Usertexte rechts  -->
                <VerticalStackLayout
                    Grid.Column="3"
                    Margin="0"
                    HorizontalOptions="End"
                    Spacing="0"
                    VerticalOptions="Center">

                    <Label
                        x:Name="lb_LoginUser"
                        Margin="0"
                        FontSize="14"
                        HorizontalOptions="Fill"
                        HorizontalTextAlignment="End"
                        LineBreakMode="TailTruncation"
                        Text=""
                        TextColor="#ffffff"
                        VerticalOptions="Center" />

                    <Label
                        x:Name="lb_LoginCustomer"
                        Margin="0"
                        FontSize="10"
                        HorizontalOptions="Fill"
                        HorizontalTextAlignment="End"
                        LineBreakMode="TailTruncation"
                        Text=""
                        TextColor="#ffffff"
                        VerticalOptions="Center" />
                </VerticalStackLayout>

            </Grid>


            <BoxView
                Margin="0,0,0,0"
                BackgroundColor="Gray"
                HeightRequest="1"
                HorizontalOptions="FillAndExpand"
                VerticalOptions="Start" />


            <StackLayout
                Margin="0"
                Padding="0"
                BackgroundColor="#34000000"
                HorizontalOptions="FillAndExpand"
                Orientation="Vertical"
                Spacing="0"
                VerticalOptions="Start">

                <StackLayout
                    x:Name="lastBuilding_Container"
                    Margin="0"
                    Padding="10,0,10,0"
                    HorizontalOptions="FillAndExpand"
                    IsVisible="False"
                    Orientation="Horizontal"
                    Spacing="0">
                    <StackLayout
                        Margin="0"
                        Padding="0,0,10,0"
                        HorizontalOptions="FillAndExpand"
                        Orientation="Vertical"
                        Spacing="0">

                        <StackLayout
                            Margin="0"
                            Padding="0"
                            HorizontalOptions="FillAndExpand"
                            Orientation="Horizontal"
                            Spacing="0">

                            <StackLayout
                                Margin="0"
                                Padding="0"
                                HorizontalOptions="FillAndExpand"
                                Orientation="Vertical"
                                Spacing="0">
                                <Label
                                    Margin="0"
                                    FontSize="11"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="Start"
                                    Text="Letztes Objekt:"
                                    TextColor="#aaaaaa"
                                    VerticalOptions="Center" />
                                <Label
                                    x:Name="last_building_name"
                                    Margin="0,-2,0,0"
                                    Padding="0"
                                    FontSize="13"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="Start"
                                    Text=""
                                    TextColor="#ffffff"
                                    VerticalOptions="Center" />
                            </StackLayout>

                            <Border
                                x:Name="btn_objektinfo_container"
                                Margin="2,0,0,0"
                                Padding="1"
                                BackgroundColor="Transparent"
                                HorizontalOptions="End"
                                IsVisible="True"
                                VerticalOptions="Start">
                                <Border.Shadow>
                                    <Shadow
                                        Brush="Black"
                                        Opacity="0.3"
                                        Radius="5"
                                        Offset="2,2" />
                                </Border.Shadow>
                                <StackLayout
                                    x:Name="btn_objektinfo"
                                    Margin="0"
                                    Padding="0"
                                    BackgroundColor="Transparent"
                                    HeightRequest="26"
                                    HorizontalOptions="End"
                                    Spacing="0"
                                    VerticalOptions="CenterAndExpand"
                                    WidthRequest="26">
                                    <Image
                                        x:Name="btn_objektinfo_img"
                                        Margin="0,0,0,0"
                                        HeightRequest="26"
                                        HorizontalOptions="CenterAndExpand"
                                        VerticalOptions="CenterAndExpand"
                                        WidthRequest="26" />
                                </StackLayout>
                            </Border>
                        </StackLayout>
                        <StackLayout
                            Margin="0"
                            Padding="0"
                            HorizontalOptions="FillAndExpand"
                            Orientation="Vertical"
                            Spacing="0">
                            <Label
                                x:Name="last_building_address"
                                Margin="0,-2,0,0"
                                Padding="0"
                                FontSize="13"
                                HorizontalOptions="FillAndExpand"
                                HorizontalTextAlignment="Start"
                                Text=""
                                TextColor="#ffffff"
                                VerticalOptions="Center" />
                            <Label
                                x:Name="last_building_zip_city"
                                Margin="0,-2,0,0"
                                Padding="0"
                                FontSize="13"
                                HorizontalOptions="FillAndExpand"
                                HorizontalTextAlignment="Start"
                                Text=""
                                TextColor="#ffffff"
                                VerticalOptions="Center" />
                        </StackLayout>
                    </StackLayout>
                    <Border
                        Margin="2,0,3,0"
                        Padding="1"
                        BackgroundColor="#041d43"
                        HorizontalOptions="End"
                        VerticalOptions="Start">
                        <Border.Shadow>
                            <Shadow
                                Brush="Black"
                                Opacity="0.3"
                                Radius="5"
                                Offset="2,2" />
                        </Border.Shadow>
                        <StackLayout
                            x:Name="btn_alertmessage_container"
                            Margin="0"
                            Padding="0"
                            BackgroundColor="#042d53"
                            HeightRequest="40"
                            HorizontalOptions="End"
                            Spacing="0"
                            VerticalOptions="CenterAndExpand"
                            WidthRequest="40">
                            <Image
                                x:Name="btn_alertmessage_img"
                                Margin="0,0,0,0"
                                HeightRequest="30"
                                HorizontalOptions="CenterAndExpand"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                        </StackLayout>
                    </Border>
                    <Border
                        Margin="3,0,3,0"
                        Padding="1"
                        BackgroundColor="#041d43"
                        HorizontalOptions="End"
                        VerticalOptions="Start">
                        <Border.Shadow>
                            <Shadow
                                Brush="Black"
                                Opacity="0.3"
                                Radius="5"
                                Offset="2,2" />
                        </Border.Shadow>
                        <StackLayout
                            x:Name="btn_message_container"
                            Margin="0"
                            Padding="0"
                            BackgroundColor="#042d53"
                            HeightRequest="40"
                            HorizontalOptions="End"
                            Spacing="0"
                            VerticalOptions="CenterAndExpand"
                            WidthRequest="40">
                            <Image
                                x:Name="btn_message_img"
                                Margin="0,0,0,0"
                                HeightRequest="30"
                                HorizontalOptions="CenterAndExpand"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                        </StackLayout>
                    </Border>
                    <Border
                        Margin="3,0,3,0"
                        Padding="1"
                        BackgroundColor="#041d43"
                        HorizontalOptions="End"
                        IsVisible="True"
                        VerticalOptions="Start">
                        <Border.Shadow>
                            <Shadow
                                Brush="Black"
                                Opacity="0.3"
                                Radius="5"
                                Offset="2,2" />
                        </Border.Shadow>
                        <StackLayout
                            x:Name="btn_objvalues_container"
                            Margin="0"
                            Padding="0"
                            BackgroundColor="#042d53"
                            HeightRequest="40"
                            HorizontalOptions="End"
                            Spacing="0"
                            VerticalOptions="CenterAndExpand"
                            WidthRequest="40">
                            <Image
                                x:Name="btn_objvalues_img"
                                Margin="0,0,0,0"
                                HeightRequest="30"
                                HorizontalOptions="CenterAndExpand"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                        </StackLayout>
                    </Border>

                    <Border
                        Margin="3,0,0,0"
                        Padding="1"
                        BackgroundColor="#041d43"
                        HorizontalOptions="End"
                        VerticalOptions="Start">
                        <Border.Shadow>
                            <Shadow
                                Brush="Black"
                                Opacity="0.3"
                                Radius="5"
                                Offset="2,2" />
                        </Border.Shadow>
                        <StackLayout
                            x:Name="btn_buildingout_container"
                            Margin="0"
                            Padding="0"
                            BackgroundColor="#042d53"
                            HeightRequest="40"
                            HorizontalOptions="End"
                            Spacing="0"
                            VerticalOptions="CenterAndExpand"
                            WidthRequest="40">
                            <Image
                                x:Name="btn_buildingout_img2"
                                Margin="0,0,0,0"
                                HeightRequest="30"
                                HorizontalOptions="CenterAndExpand"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                        </StackLayout>
                    </Border>
                </StackLayout>


                <Border
                    x:Name="btn_buildingorder_container"
                    Margin="10,1,10,2"
                    Padding="1"
                    BackgroundColor="#041d43"
                    HorizontalOptions="FillAndExpand"
                    IsVisible="False">
                    <Border.Shadow>
                        <Shadow
                            Brush="Black"
                            Opacity="0.3"
                            Radius="5"
                            Offset="2,2" />
                    </Border.Shadow>
                    <StackLayout
                        x:Name="btn_buildingorder"
                        Margin="0"
                        Padding="5"
                        BackgroundColor="#042d53"
                        HeightRequest="40"
                        HorizontalOptions="FillAndExpand">
                        <StackLayout
                            Margin="10,0,0,0"
                            Padding="0"
                            HeightRequest="30"
                            HorizontalOptions="FillAndExpand"
                            Orientation="Horizontal"
                            Spacing="0"
                            VerticalOptions="CenterAndExpand">
                            <Image
                                x:Name="btn_buildingorder_limg"
                                Margin="0,0,15,0"
                                HeightRequest="30"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                            <StackLayout
                                Margin="0,-4,0,0"
                                Padding="0"
                                HorizontalOptions="FillAndExpand"
                                Orientation="Vertical"
                                Spacing="0"
                                VerticalOptions="CenterAndExpand">
                                <Label
                                    Margin="0"
                                    Padding="0"
                                    FontSize="18"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="Start"
                                    Text="Arbeiten auswählen"
                                    TextColor="#ffffff" />
                                <Label
                                    Margin="0,-3,0,0"
                                    Padding="0"
                                    FontSize="12"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="Start"
                                    Text="zum ausgewählten Objekt"
                                    TextColor="#aaaaaa" />
                            </StackLayout>
                            <Border
                                x:Name="btn_buildingorderToTime_count"
                                Margin="0"
                                Padding="5"
                                BackgroundColor="#ff0000"
                                HorizontalOptions="End"
                                IsVisible="False"
                                StrokeShape="RoundRectangle 5">
                                <Border.Shadow>
                                    <Shadow
                                        Brush="Black"
                                        Opacity="0.3"
                                        Radius="5"
                                        Offset="2,2" />
                                </Border.Shadow>
                                <Label
                                    x:Name="btn_buildingorderToTime_counttext"
                                    Margin="0"
                                    Padding="0"
                                    FontSize="16"
                                    HorizontalOptions="Center"
                                    HorizontalTextAlignment="Center"
                                    Text="13"
                                    TextColor="#ffffff"
                                    VerticalOptions="Center" />
                            </Border>
                            <Image
                                x:Name="btn_buildingorder_rimg"
                                Margin="10,0,0,0"
                                HeightRequest="30"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                        </StackLayout>
                    </StackLayout>
                </Border>

                <Border
                    x:Name="btn_inwork_container"
                    Margin="10,10,10,15"
                    Padding="1"
                    BackgroundColor="#04431d"
                    HorizontalOptions="FillAndExpand"
                    IsVisible="False">
                    <Border.Shadow>
                        <Shadow
                            Brush="Black"
                            Opacity="0.3"
                            Radius="5"
                            Offset="2,2" />
                    </Border.Shadow>
                    <StackLayout
                        x:Name="btn_inwork"
                        Margin="0"
                        Padding="5"
                        BackgroundColor="#04532d"
                        HeightRequest="40"
                        HorizontalOptions="FillAndExpand">
                        <StackLayout
                            Margin="10,0,0,0"
                            Padding="0"
                            HeightRequest="30"
                            HorizontalOptions="FillAndExpand"
                            Orientation="Horizontal"
                            Spacing="0"
                            VerticalOptions="CenterAndExpand">
                            <Image
                                x:Name="btn_inwork_limg"
                                Margin="0,0,15,0"
                                HeightRequest="30"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                            <StackLayout
                                Margin="0,-4,0,0"
                                Padding="0"
                                HorizontalOptions="StartAndExpand"
                                Orientation="Vertical"
                                Spacing="0"
                                VerticalOptions="CenterAndExpand">
                                <Label
                                    Margin="0"
                                    Padding="0"
                                    FontSize="18"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="Start"
                                    Text="Aktuelle Arbeiten"
                                    TextColor="#ffffff" />
                                <Label
                                    Margin="0,-3,0,0"
                                    Padding="0"
                                    FontSize="12"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="Start"
                                    Text="Hier sind Sie gerade dabei"
                                    TextColor="#aaaaaa" />
                            </StackLayout>
                            <Border
                                Margin="0"
                                Padding="5"
                                BackgroundColor="#009900"
                                HorizontalOptions="End"
                                StrokeShape="RoundRectangle 5">
                                <Border.Shadow>
                                    <Shadow
                                        Brush="Black"
                                        Opacity="0.3"
                                        Radius="5"
                                        Offset="2,2" />
                                </Border.Shadow>
                                <Label
                                    x:Name="inwork_start_count_text"
                                    Margin="0"
                                    Padding="0"
                                    FontSize="16"
                                    HorizontalOptions="Center"
                                    HorizontalTextAlignment="Center"
                                    Text="1"
                                    TextColor="#ffffff"
                                    VerticalOptions="Center" />
                            </Border>
                            <StackLayout
                                Margin="0,-4,0,0"
                                Padding="0"
                                HorizontalOptions="EndAndExpand"
                                Orientation="Vertical"
                                Spacing="0"
                                VerticalOptions="CenterAndExpand">
                                <Label
                                    Margin="0,0,0,0"
                                    Padding="0"
                                    FontSize="12"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="Start"
                                    Text="Seit"
                                    TextColor="#aaaaaa" />
                                <Label
                                    x:Name="inwork_starttime_text"
                                    Margin="0,-3,0,0"
                                    Padding="0"
                                    FontSize="20"
                                    HorizontalOptions="FillAndExpand"
                                    HorizontalTextAlignment="End"
                                    Text="00:00"
                                    TextColor="#ffcc00" />
                            </StackLayout>
                        </StackLayout>
                    </StackLayout>
                </Border>


                <Image
                    x:Name="btn_nachbuchen_img"
                    Margin="5,-19,0,-60"
                    BindingContext="{x:Reference btn_nachbuchen_container}"
                    HeightRequest="70"
                    HorizontalOptions="Start"
                    IsVisible="{Binding Path=IsVisible}"
                    VerticalOptions="Start"
                    WidthRequest="90" />

                <Border
                    x:Name="btn_nachbuchen_container"
                    Margin="70,10,10,8"
                    Padding="1"
                    BackgroundColor="#041d43"
                    HorizontalOptions="FillAndExpand"
                    IsVisible="False">
                    <Border.Shadow>
                        <Shadow
                            Brush="Black"
                            Opacity="0.3"
                            Radius="5"
                            Offset="2,2" />
                    </Border.Shadow>
                    <StackLayout
                        x:Name="btn_nachbuchen"
                        Margin="0"
                        Padding="5"
                        BackgroundColor="#042d53"
                        HeightRequest="40"
                        HorizontalOptions="FillAndExpand">
                        <StackLayout
                            Margin="10,0,0,0"
                            Padding="0"
                            HeightRequest="30"
                            HorizontalOptions="FillAndExpand"
                            Orientation="Horizontal"
                            Spacing="0"
                            VerticalOptions="CenterAndExpand">
                            <Image
                                x:Name="btn_posnachbuchen_limg"
                                Margin="0,-3,0,0"
                                HeightRequest="30"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                            <Image
                                x:Name="btn_produktenachbuchen_limg"
                                Margin="-10,3,10,0"
                                HeightRequest="30"
                                VerticalOptions="CenterAndExpand"
                                WidthRequest="30" />
                            <StackLayout
                                Margin="0,-4,0,0"
